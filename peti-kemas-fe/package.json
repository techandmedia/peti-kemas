{
  "name": "with-ant-design",
  "version": "1.0.0",
  "scripts": {
    "format": "prettier --write \"next.config.js\" \"components/**/*.js\" \"modules/**/*.js\" \"utils/**/*.js\" \"pages/**/*.js\" \"test/**/*.js\"",
    "dev": "yarn clean && cross-env NODE_ENV=development next",
    "build": "yarn clean && cross-env NODE_ENV=production next build",
    "export": "next export",
    "start": "NODE_ENV=production next start",
    "reset": "rm -rf node_modules .next && yarn",
    "clean": "rm -rf .next && yarn format",
    "ssr-windows": "cross-env NODE_ENV=production && node server/production-server.js",
    "ssr-linux": "cross-env NODE_ENV=production node server/production-server.js"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "antd": "^3.26.7",
    "babel-plugin-import": "^1.13.0",
    "next": "^9.2.0",
    "null-loader": "^3.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^6.0.3",
    "prettier": "^1.19.1"
  }
}
